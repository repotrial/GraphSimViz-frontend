    "vue-router": "^3.5.3",
    "vuetify": "^2.5.10"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@vue/cli-plugin-babel": "^5.0.4",
    "@vue/cli-plugin-eslint": "^5.0.4",
